{
  "presets": [["@babel/preset-env", { "loose": true, "modules": false }], "@babel/preset-react"],
  "plugins": [["@babel/proposal-class-properties", { "loose": true }], ["transform-react-remove-prop-types", { "removeImport": true }]],
  "env": {
    "test": {
      "plugins": ["@babel/transform-modules-commonjs", "@babel/plugin-transform-runtime"]
    }
  }
}
